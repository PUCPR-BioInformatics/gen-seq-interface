{
    "name": "gen-seq-interface",
    "version": "1.0.0-beta.1",
    "scripts": {
        "ng": "ng",
        "analyse": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/stats-es2015.json",
        "serve": "ng serve",
        "build": "ng build  --configuration production",
        "build-qa": "ng build --configuration qa",
        "build-development": "yarn lint-and-test && ng build --configuration development",
        "test": "ng test",
        "test-coverage": "ng test --no-watch --code-coverage",
        "test-coverage-watch": "ng test --code-coverage",
        "lint": "ng lint && yarn lint-style",
        "lint-and-test": "yarn lint && ng test --no-watch --browsers=ChromeHeadless",
        "lint-style": "stylelint \"src/**/*.scss\"",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~12.0.0",
